<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dows.auth.crud.dao.OauthRedirectUriDao">

    <resultMap type="org.dows.auth.crud.entity.OauthRedirectUri" id="OauthRedirectUriMap">
        <result property="id" column="id" jdbcType="INTEGER"/>
        <result property="clientId" column="client_id" jdbcType="VARCHAR"/>
        <result property="redirectUri" column="redirect_uri" jdbcType="VARCHAR"/>
        <result property="dt" column="dt" jdbcType="TIMESTAMP"/>
        <result property="deleted" column="deleted" jdbcType="BOOLEAN"/>
    </resultMap>

    <!-- 批量插入 -->
    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">
        insert into dows_uim.oauth_redirect_uri(client_id, redirect_uri, dt, deleted)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.clientId}, #{entity.redirectUri}, #{entity.dt}, #{entity.deleted})
        </foreach>
    </insert>
    <!-- 批量插入或按主键更新 -->
    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">
        insert into dows_uim.oauth_redirect_uri(client_id, redirect_uri, dt, deleted)
        values
        <foreach collection="entities" item="entity" separator=",">
            (#{entity.clientId}, #{entity.redirectUri}, #{entity.dt}, #{entity.deleted})
        </foreach>
        on duplicate key update
        client_id = values(client_id) , redirect_uri = values(redirect_uri) , dt = values(dt) , deleted =
        values(deleted)
    </insert>

</mapper>

